{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.5.2",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build && esno ./scripts/generate-sitemap.ts",
    "dev": "nodemon",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./pages ./components ./lib ./layouts ./scripts",
    "prepare": "husky install",
    "prettier": "prettier --check --write \"(pages|components|lib|layouts|scripts)/**/*\"",
    "serve": "next start",
    "start": "cross-env SOCKET=true esno ./scripts/next-remote-watch.ts ./data"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,mdx,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource/inter": "4.5.2",
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react-dom": "18",
    "autoprefixer": "^10.4.0",
    "esbuild": "^0.13.13",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "mdx-bundler": "^8.0.0",
    "next": "14",
    "next-themes": "^0.0.14",
    "postcss": "^8.4.5",
    "preact": "^10.6.2",
    "react": "18",
    "react-dom": "18",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.2.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-mdx": "^2.3.0",
    "sharp": "^0.28.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^3.0.18",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.9",
    "@prettier/plugin-xml": "^2.0.1",
    "@svgr/webpack": "^6.1.2",
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/prettier": "^2.4.4",
    "@types/react": "18",
    "@typescript-eslint/parser": "^5.15.0",
    "@umijs/fabric": "^2.10.2",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "commander": "^9.1.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "14",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "esno": "^0.14.1",
    "express": "^4.17.3",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^6.0.0",
    "inquirer": "^8.1.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "mdast-util-to-string": "^3.1.0",
    "next-remote-watch": "^1.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "typescript": "^4.6.2",
    "vfile": "^5.3.2"
  }
}
